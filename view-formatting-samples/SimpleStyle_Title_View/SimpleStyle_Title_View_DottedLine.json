{
    "hideColumnHeader": true,
    "hideSelection": true,
    "rowFormatter": {
      "elmType": "div",
      "style": {
        "width": "50%",
        "margin": "10px 0px 15px 20px",
        "padding": "0",
        "border-style": "none"
      },
      "children": [
        {
          "elmType": "a",
          "style":{
            "text-decoration":"none"
          },
          "attributes": {
            "target":"_blank",
            "href": "[$URL]"
          },
          "children": [
            {
              "elmType": "div",
              "txtContent": "[$Title]",
              "style": {
                "width":"400px",
                "box-shadow": "0px 0px 0px 5px #dfefff",
                "border": "dashed 2px white",
                "padding": "10px 10px 5px 10px",
                "background-color": "#dfefff",
                "font-size":"20px",
                "color":"#000000"
              }
           }
          ]
        }
      ]
    }
  }